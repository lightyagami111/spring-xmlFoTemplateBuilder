${#this.block-external-graphic}
    <fo:block ${#this.attributes} ${.} ${/this.attributes}>
        <fo:external-graphic src="${this.external-graphic.src}" ${#this.external-graphic.attributes} ${.} ${/this.external-graphic.attributes}/>
    </fo:block>
${/this.block-external-graphic}

${#this.block-container-external-graphic}
<fo:block-container margin-top="-7.5mm">
    <fo:block  text-align="left" margin-left="-4.5mm">
        <fo:external-graphic src="${this.external-graphic.src}" height="28.2mm" width="31mm" content-height="scale-to-fit" scaling="non-uniform"/>
    </fo:block>
</fo:block-container>
${/this.block-container-external-graphic}

${#this.block-container-external-graphic-leader}
<fo:block-container>
    <fo:block margin-top="1.1mm" margin-left="0.2mm">
        <fo:external-graphic src="${this.external-graphic.src}" height="8.6mm" width="3.95cm" content-height="scale-to-fit" scaling="non-uniform"/>
    </fo:block>
    ${> blocks-leader}
</fo:block-container>
${/this.block-container-external-graphic-leader}