<fo:page-sequence master-reference="%{master-reference}">
    %{#static-content}
        <fo:static-content flow-name="%{flow-name}">
            ${#%{flow-name}}
                ${> xsl-region}
            ${/%{flow-name}}
        </fo:static-content>
    %{/static-content}

    <fo:flow flow-name="xsl-region-body">
        ${#xsl-region-body}
            ${> xsl-region}
        ${/xsl-region-body}
    </fo:flow>

</fo:page-sequence>